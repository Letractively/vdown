msgstr "Download #%(number)s beendet"

#: gui.py:259
#, python-format
msgid "Download #%(number)s failed"
msgstr "Download #%(number)s gescheitert"

#: gui.py:262
#, python-format
msgid "%(successful)s of %(all)s successful"
msgstr "%(successful)s von %(all)s erfolgreich"

